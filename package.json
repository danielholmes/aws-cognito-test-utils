{
  "name": "@dhau/aws-cognito-test-utils",
  "version": "0.0.25",
  "type": "module",
  "description": "MSW handlers and other utils for AWS Cognito",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier -w \"src/**/*.ts\"",
    "format:check": "prettier -c \"src/**/*.ts\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --ext .ts --fix ./src",
    "lint:check": "eslint --cache --cache-location ./node_modules/.cache/eslint --ext .ts ./src",
    "ts": "tsc -p tsconfig.esm.json --noEmit",
    "test": "node --watch --import tsx --test src/*.spec.ts",
    "test:all": "node --import tsx --test src/*.spec.ts",
    "prebuild": "run-p format:check ts lint:check test:all",
    "build:esm": "rollup --config rollup.config.js",
    "build:ts": "tsc -p tsconfig.types.json",
    "build": "npm-run-all -s clean -p build:*",
    "npm:publish": "npm publish --access public",
    "deploy": "run-s build npm:publish",
    "prepare": "husky install"
  },
  "author": "Daniel Holmes",
  "license": "MIT",
  "dependencies": {
    "@dhau/msw-builders": "^0.0.28",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "amazon-cognito-identity-js": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.0.1",
    "msw": "^2.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rollup": "^4.1.4",
    "tsx": "^4.9.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "amazon-cognito-identity-js": ">=6.0.0",
    "msw": "^2.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
