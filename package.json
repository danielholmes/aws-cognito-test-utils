{
  "name": "@dhau/aws-cognito-test-utils",
  "version": "0.0.18",
  "type": "module",
  "description": "MSW handlers for AWS Cognito",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier -w \"src/**/*.ts\"",
    "format:check": "prettier -c \"src/**/*.ts\"",
    "ts": "tsc -p tsconfig.esm.json --noEmit",
    "prebuild": "run-p format:check ts",
    "build:esm": "rollup --config rollup.config.js",
    "build:ts": "tsc -p tsconfig.types.json",
    "build": "npm-run-all -s clean -p build:*",
    "npm:publish": "npm publish --access public",
    "deploy": "run-s build npm:publish",
    "prepare": "husky install"
  },
  "author": "Daniel Holmes",
  "license": "MIT",
  "dependencies": {
    "@dhau/msw-builders": "^0.0.17",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^20.8.6",
    "amazon-cognito-identity-js": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "msw": "^2.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rollup": "^4.1.4",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "amazon-cognito-identity-js": ">=6.0.0",
    "msw": "^2.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
