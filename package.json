{
  "name": "@dhau/aws-cognito-test-utils",
  "version": "0.0.4",
  "type": "module",
  "description": "MSW handlers for AWS Cognito",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier -w \"src/**/*.ts\"",
    "format:check": "prettier -c \"src/**/*.ts\"",
    "ts": "tsc -p tsconfig.esm.json --noEmit",
    "prebuild": "run-p format:check test:all ts",
    "postbuild": "./scripts/cjs-extensions.sh",
    "test": "mocha -w",
    "test:all": "echo 'TODO: mocha'",
    "build:esm": "rollup --config rollup.config.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm-run-all -s clean -p build:*",
    "npm:publish": "npm publish --access public",
    "deploy": "run-s build npm:publish",
    "prepare": "husky install"
  },
  "author": "Daniel Holmes",
  "license": "MIT",
  "dependencies": {
    "@dhau/msw-builders": "^0.0.15",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/chai": "^4.3.4",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.1",
    "amazon-cognito-identity-js": "^6.0.0",
    "chai": "^4.3.7",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "msw": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rollup": "^3.10.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "amazon-cognito-identity-js": ">=6.0.0",
    "msw": ">=0.27.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
